<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:width="10dp" android:height="40dp" android:right="6dp">
        <shape>
            <solid android:color="@android:color/holo_green_dark"/>
        </shape>
    </item>
    <item android:width="9dp" android:height="8dp"
        android:top="45dp">
        <rotate android:fromDegrees="180"
            android:pivotX="0"
            android:pivotY="0">
            <shape>
                <solid android:color="@android:color/holo_green_dark"/>
            </shape>
        </rotate>
    </item>

</layer-list>